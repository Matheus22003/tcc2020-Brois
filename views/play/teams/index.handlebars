{{#each erros}}
<div class="fixed-top mt-5 container fadeOut animated faster delay-5s">
    <div class="alert alert-danger mt-5 bounceInDown animated " role="alert">{{texto}}</div>
</div>
{{else}}
{{/each}}

<div class="container-fluid" id="conteudoPagina">

    <div class="border-title ">
        <h7 class="text-light pl-1 ml-2" id="home">Play / Teams</h7>
        <h1 class="text-light pl-1 ml-2">TEAMS</h1>
    </div>
    <div class="box">
        <button type="button" class="btn btn_time mb-3 text-light">CREATE TEAMS</button>
    </div>
    <div class="container-fluid bg-252525 " id="teamCreate">
        <div class="row text-center p-3">
            <h3 class="col-4 text-light" style="font-size: 2.1vh;">Name team</h3>
            <h3 class="col-4 text-light" style="font-size: 2.1vh;">Game</h3>
            <h3 class="col-4 text-light" style="font-size: 2.1vh;">Options</h3>
        </div>

    </div>

    <div class=" mt-3 times container-fluid ">
        <div class="bg-252525 row text-center p-3 container-fluid mx-auto">
            <h3 class="col-4 text-light" style="font-size: 2vh;">INTZ E-SPORTS (INTZ)</h3>
            <h3 class="col-4 text-light" style="font-size: 2vh;">CSGO</h3>
            <h3 class="col-4 text-light" style="font-size: 2vh;">VISIT TEAM PROFILE <span class="mr-5 ml-1"></span> EDIT TEAM</h3>
            

        </div>

    </div>

</div>
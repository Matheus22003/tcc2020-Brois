    <div class="bg-user" id="level">
        <h6 class="lvl p-1">30</h6>
    </div>
    {{!-- Assinatura --}}
    <div class="bg-user pl-3" id="assinatura">
        <img class="" src="/images/icones-usuario/ranking.svg" alt="">
    </div>
    {{!-- divis√£o --}}
    <div class="bg-user pl-2" id="separador">
        <h2 class="cinza"> | </h2>
    </div>

{{#if user}}

<div style="right: 0;" class="jumbotron-fluid d-flex flex-row position-absolute mt-2">
    <div class="bg-transparent my-auto mr-n4" id="imagePerfil">
        <img src="/user_pictures/perfil_{{#each user}}{{_id}}{{#each user}}{{_id}}{{else}}{{/each}}{{else}}{{/each}}.png"
            class="rounded-circle" alt="">
    </div>
    {{!-- Gambiarra --}}
    <div class="bg-user" id="espacamentoUser"></div>
    {{!-- Dados --}}
    <div class="bg-user">
        <h6 class="text-light font-play pt-1">{{#each user}}{{nome}}{{else}}{{/each}}</h6>
        <h6 class="text-light font-play mt-n2 cor-secundaria-site" id="dinheiro">R$
            {{#each user}}{{#each carteiras}}{{reais}}{{else}}{{/each}}{{else}}{{/each}},00
        </h6>
